<h1>Editing Element</h1>

<% form_for @element, :html => { :multipart => true } do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :attachment %><br />
    <%= f.file_field :attachment %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @element %> |
<%= link_to 'Back', elements_path %>