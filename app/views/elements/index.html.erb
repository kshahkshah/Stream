<h1>Listing elements</h1>

<table>
  <tr>
    <th>Attachment file name</th>
    <th>Attachment content type</th>
    <th>Attachment file size</th>
    <th>Attachment updated at</th>
  </tr>

<% @elements.each do |element| %>
  <tr>
    <td><%=h element.attachment_file_name %></td>
    <td><%=h element.attachment_content_type %></td>
    <td><%=h element.attachment_file_size %></td>
    <td><%=h element.attachment_updated_at %></td>
    <td><%= link_to 'Show', element %></td>
    <td><%= link_to 'Edit', edit_element_path(element) %></td>
    <td><%= link_to 'Destroy', element, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New element', new_element_path %>